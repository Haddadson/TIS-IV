@{
    ViewBag.Title = "Home Page";
}
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Form Components | Notika - Notika Admin Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- favicon
    ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <!-- Google Fonts
    ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- font awesome CSS
    ============================================ -->
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <!-- owl.carousel CSS
    ============================================ -->
    <link rel="stylesheet" href="css/owl.carousel.css">
    <link rel="stylesheet" href="css/owl.theme.css">
    <link rel="stylesheet" href="css/owl.transitions.css">
    <!-- meanmenu CSS
    ============================================ -->
    <link rel="stylesheet" href="css/meanmenu/meanmenu.min.css">
    <!-- animate CSS
    ============================================ -->
    <link rel="stylesheet" href="css/animate.css">
    <!-- summernote CSS
    ============================================ -->
    <link rel="stylesheet" href="css/summernote/summernote.css">
    <!-- Range Slider CSS
    ============================================ -->
    <link rel="stylesheet" href="css/themesaller-forms.css">
    <!-- normalize CSS
    ============================================ -->
    <link rel="stylesheet" href="css/normalize.css">
    <!-- mCustomScrollbar CSS
    ============================================ -->
    <link rel="stylesheet" href="css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- Notika icon CSS
    ============================================ -->
    <link rel="stylesheet" href="css/notika-custom-icon.css">
    <!-- bootstrap select CSS
    ============================================ -->
    <link rel="stylesheet" href="css/bootstrap-select/bootstrap-select.css">
    <!-- datapicker CSS
    ============================================ -->
    <link rel="stylesheet" href="css/datapicker/datepicker3.css">
    <!-- Color Picker CSS
    ============================================ -->
    <link rel="stylesheet" href="css/color-picker/farbtastic.css">
    <!-- main CSS
    ============================================ -->
    <link rel="stylesheet" href="css/chosen/chosen.css">
    <!-- notification CSS
    ============================================ -->
    <link rel="stylesheet" href="css/notification/notification.css">
    <!-- dropzone CSS
    ============================================ -->
    <link rel="stylesheet" href="css/dropzone/dropzone.css">
    <!-- wave CSS
    ============================================ -->
    <link rel="stylesheet" href="css/wave/waves.min.css">
    <!-- main CSS
    ============================================ -->
    <link rel="stylesheet" href="css/main.css">
    <!-- style CSS
    ============================================ -->
    <link rel="stylesheet" href="style.css">
    <!-- responsive CSS
    ============================================ -->
    <link rel="stylesheet" href="css/responsive.css">
</head>
<div>
    <div>
        <h3>Cadastrar uma nova tabela</h3>
        <h5> Digite os campos abaixo para criar uma nova tabela</h5>
    </div>
    <article>
        @using (Html.BeginForm("ShowClients"))
        {
            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                <div class="form-example-int form-horizental">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">SGDP</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm" id="sgdp">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Analista Responsável</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Municipio(s)</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <select class="form-control" id="municipios">
                                        <option>Selecione</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Data geração da Tabela</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm" name="data-geracao" id="data-geracao">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Ano(s) referentes</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm" id="ano-referente">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Título Aba 1 - NFs x ANP</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Título Aba 2 - Cupons Fiscais</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-example-int form-horizental mg-t-15">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                                <label class="hrzn-fm">Título Aba 3 - Outras Informações</label>
                            </div>
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-12">
                                <div class="nk-int-st">
                                    <input type="text" class="form-control input-sm">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
            </div>
            <div class="button-icon-btn">
                <button class="btn btn-default btn-icon-notika" id="cadastrar-tabela"><i class="notika-icon notika-checked"></i> Enviar</button>
            </div>
        </article>
    <div class="col-md-3">
        <h2>Teste Obter Excel</h2>
        <button id="obter-excel">Obter Excel</button>
    </div>
</div>

@section JavascriptPagina {
    <script type="text/javascript" src="~/Scripts/moment.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Util/vanilla-masker.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Util/form-field-handler.js"></script>
    <script type="text/javascript" src="~/Scripts/ObterExcel/obterExcel.js"></script>
    <script type="text/javascript" src="~/Scripts/Municipios/municipio.js"></script>
    <script type="text/javascript" src="~/Scripts/TabelaUsuario/cadastrarTabela.js"></script>
    <script type="text/javascript">
        var urlObterExcel       = '@Url.Action("ObterExcel", "Home")',
            urlListarMunicipios = '@Url.Action("ListarMunicipiosAnp", "Home")',
            urlTabelaUsuario    = '@Url.Action("cadastrarTabela", "TabelaUsuario", new { AnoReferente = ?, SGDP = ? } )'

    </script>
}
