<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/owl.theme.css">
    <link rel="stylesheet" href="~/Content/owl.transitions.css">
    <!-- meanmenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/meanmenu/meanmenu.min.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/normalize.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- jvectormap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/jvectormap/jquery-jvectormap-2.0.3.css">
    <!-- notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/notika-custom-icon.css">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/wave/waves.min.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/main.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/style.css">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/Content/responsive.css">

    <link rel="stylesheet" href="~/Content/Site.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/Scripts/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink(" ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li>@Html.ActionLink("Nota Fiscal", "Index", "NotaFiscal")</li>
                    <li>@Html.ActionLink("Listar Tabelas", "Index", "TabelaUsuario")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container-fluid body-content text-center">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/ScriptsValidacaoNotas")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("JavaScriptPagina", required: false)
    <!-- jquery
    ============================================ -->
    <script defer src="~/Scripts/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script defer src="~/Scripts/bootstrap.min.js"></script>
    <!-- wow JS
        ============================================ -->
    <script defer src="~/Scripts/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script defer src="~/Scripts/jquery-price-slider.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script defer src="~/Scripts/owl.carousel.min.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script defer src="~/Scripts/jquery.scrollUp.min.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script defer src="~/Scripts/meanmenu/jquery.meanmenu.js"></script>
    <!-- counterup JS
        ============================================ -->
    <script defer src="~/Scripts/counterup/jquery.counterup.min.js"></script>
    <script defer src="~/Scripts/counterup/waypoints.min.js"></script>
    <script defer src="~/Scripts/counterup/counterup-active.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script defer src="~/Scripts/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- jvectormap JS
        ============================================ -->
    <script defer src="~/Scripts/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script defer src="~/Scripts/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script defer src="~/Scripts/jvectormap/jvectormap-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script defer src="~/Scripts/sparkline/jquery.sparkline.min.js"></script>
    <script defer src="~/Scripts/sparkline/sparkline-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script defer src="~/Scripts/flot/jquery.flot.js"></script>
    <script defer src="~/Scripts/flot/jquery.flot.resize.js"></script>
    <script defer src="~/Scripts/flot/curvedLines.js"></script>
    <script defer src="~/Scripts/flot/flot-active.js"></script>
    <!-- knob JS
        ============================================ -->
    <script defer src="~/Scripts/knob/jquery.knob.js"></script>
    <script defer src="~/Scripts/knob/jquery.appear.js"></script>
    <script defer src="~/Scripts/knob/knob-active.js"></script>
    <!--  wave JS
        ============================================ -->
    <script defer src="~/Scripts/wave/waves.min.js"></script>
    <script defer src="~/Scripts/wave/wave-active.js"></script>
    
    <!--  todo JS
        ============================================ -->
    <script defer src="~/Scripts/todo/jquery.todo.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script defer src="~/Scripts/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script defer src="~/Scripts/main.js"></script>
    <script type="text/javascript" defer src="~/Scripts/moment.min.js"></script>
    <script type="text/javascript" defer src="~/Scripts/Util/vanilla-masker.min.js"></script>
    <script type="text/javascript" defer src="~/Scripts/Util/form-field-handler.js"></script>
</body>
</html>
