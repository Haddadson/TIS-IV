
@{
    ViewBag.Title = "Escolher Tabela";
}

<div class="mg-t-15">
    <div class="form-example-wrap">
        <div class="col-md-12">
            <h3>Escolher Tabela</h3>
        </div>
        <div class="col-md-12">
            <div class="form-example-int form-horizental">
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-4 col-md-3 col-sm-3 col-xs-12">
                            <label class="hrzn-fm">Número SGDP</label>
                        </div>
                        <div class="col-lg-7 col-md-7 col-sm-7 col-xs-11">
                            <div class="nk-int-st">
                                <select class="form-control" id="sgdp"></select>
                            </div>
                        </div>
                        <label style="float:left">*</label>
                    </div>
                </div>
            </div>
            <div class="form-example-int form-horizental">
                <div class="form-group">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <button class="btn btn-default btn-icon-notika" id="cadastrar_nota_fiscal">
                                Cadastrar Nota Fiscal
                            </button>
                            <button class="btn btn-default btn-icon-notika" id="cadastrar_cupom_fiscal">
                                Cadastrar Cupom Fiscal
                            </button>
                            <button class="btn btn-default btn-icon-notika" id="Vizualizar_tabela">
                                Vizualizar Tabela
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </div>
</div>
